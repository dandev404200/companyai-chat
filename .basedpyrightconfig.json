{
  "$schema": "https://raw.githubusercontent.com/microsoft/pyright/main/schemas/basedpyrightconfig.schema.json",
  "version": "1.1.0",
  "executionEnvironments": [
    {
      "root": "backend",
      "pythonVersion": "3.13",
      "pythonPlatform": "Linux",
      "extraPaths": [
        "backend/app",
        "backend/.venv/lib/python3.13/site-packages"
      ]
    }
  ],
  "reportTypeshedErrors": false,
  "reportMissingImports": true,
  "reportMissingTypeStubs": "none",
  "reportUnnecessaryCasts": "warning",
  "reportUnnecessaryIsInstance": "warning",
  "reportUnnecessaryComparison": "warning",
  "reportUnnecessaryContains": "warning",
  "reportUnusedVariable": "warning",
  "reportUnusedImport": "warning",
  "reportUnusedClass": "warning",
  "reportUnusedFunction": "warning",
  "reportUnusedCallResult": "warning",
  "reportDuplicateImport": "warning",
  "reportImportCycles": "none",
  "reportPrivateUsage": "none",
  "reportConstantRedefinition": "warning",
  "reportIncompatibleMethodOverride": "warning",
  "reportIncompatibleVariableOverride": "warning",
  "reportInconsistentConstructor": "warning",
  "reportOverlappingOverload": "warning",
  "reportMissingTypeArgument": "warning",
  "reportMissingParameterType": "warning",
  "reportMissingReturnType": "warning",
  "reportOptionalMemberAccess": "none",
  "reportOptionalSubscript": "none",
  "reportOptionalCall": "none",
  "reportOptionalIterable": "none",
  "reportOptionalContextManager": "none",
  "reportOptionalOperand": "none",
  "reportAny": "none",
  "reportUnknownArgumentType": "none",
  "reportUnknownLambdaType": "none",
  "reportUnknownMemberType": "none",
  "reportUnknownParameterType": "none",
  "reportUnknownVariableType": "none",
  "reportArgumentType": "none",
  "reportCallIssue": "none",
  "reportGeneralTypeIssues": "warning",
  "reportAttributeAccessIssue": "warning",
  "reportAssignmentType": "warning",
  "reportReturnType": "warning",
  "reportSelfClsParameterName": "warning",
  "reportUnboundVariable": "warning",
  "reportUndefinedVariable": "warning",
  "reportAssertAlwaysTrue": "warning",
  "reportUninitializedInstanceVariable": "warning",
  "typeCheckingMode": "standard",
  "useLibraryCodeForTypes": true,
  "analyzeUnannotatedFunctions": true,
  "enableTypeIgnoreComments": true,
  "autoImportCompletions": true,
  "indexing": true,
  "venvPath": "backend",
  "venv": ".venv",
  "pythonVersion": "3.13",
  "pythonPlatform": "Linux",
  "include": [
    "backend/**/*.py",
    "**/*.py"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.venv",
    "**/.venv/**",
    "**/node_modules",
    "**/.git",
    "**/*.pyc",
    "**/tests",
    "**/test",
    "**/test_*",
    "**/*_test.py"
  ],
  "ignore": [
    "**/__pycache__",
    "**/.venv",
    ".git",
    "**/.*",
    "**/tests",
    "**/test"
  ],
  "stubPath": "typings",
  "extraPaths": [
    "backend",
    "backend/app",
    "backend/.venv/lib/python3.13/site-packages",
    "typings",
    "typings/langchain_custom",
    "typings/runtime"
  ],
  "defineConstant": {
    "DEBUG": false,
    "PRODUCTION": true
  }
}